            "format": {
                "type": "enumerated",
                "values": [
                    {
                        "type": "value",
                        "value": 0,
                        "description": {
                            "en": "Power consumption reporting disabled",
                            "pl": ""
                        }
                    },
                    {
                        "type": "range",
