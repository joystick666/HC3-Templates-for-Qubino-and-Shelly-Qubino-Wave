                "pl": ""
            },
            "size": 1,
            "defaultValue": 1,
            "format": {
                "type": "enumerated",
                "values": [
                    {
                        "type": "value",
                        "value": 0,
                        "description": {
                            "en": "momentary switch",
                            "pl": ""
                        }
                    },
                    {
                        "type": "value",
